<Window x:Class="Client.WindowsForDeleteSpecification.DeleteSpecificationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Удалить Спецификацию" Height="200" Width="300">
    <StackPanel Margin="10">
        <StackPanel>
            <TextBlock Text="ID документа" Margin="0,0,0,5" />
            <TextBox x:Name="DocumentIdTextBox" />
        </StackPanel>
        <StackPanel>
            <TextBlock Text="ID спецификации" Margin="0,0,0,5" />
            <TextBox x:Name="SpecificationIdTextBox" />
        </StackPanel>
        <Button Content="Удалить" Click="DeleteButton_Click" />
    </StackPanel>
</Window>
